pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- bike game inspired 
-- from rabidgremlin's
-- tutorial
-- author: alan morgan

scrn = {}
p1 = {}
p2 = {}

function _init()
	show_menu()
end

function _update()
 scrn.upd()
end

function _draw()
	scrn.drw()
end

function show_menu()
 scrn.upd = update_menu
 scrn.drw = draw_menu
 sfx(4,0,0)
 sfx(5,1,0)
end

function update_menu()
 if (btnp(4)) then run_game() end
end

function draw_menu()
 cls()
 
 spr(0,0,0,20,20)
 print("press z to start",27,50,7) 
 print("player 1 - s/e/d/f  to turn",10,43,14) 
 print("player 2 - ‹”ƒ‘ to turn",10,57,12) 

end

function run_game()
 scrn.upd = update_game
 scrn.drw = draw_game
 
 p1.x = 32
 p1.y = 64
 p1.dir = 1 -- starts up
 p1.dead = false
 
 p2.x = 96
 p2.y = 64
 p2.dir = 1 -- starts up
 p2.dead = false
 
 cls() -- clear screen
 
 rect(0,0,127,127,1) -- border
 sfx(2,0,0) -- sound channel offset
 sfx(0,1,0) -- motor
end

function update_player(player,player_num)
 local last_dir = player.dir
    
 -- up, right, down, left
 if (player.dir == 1) then player.y=player.y-1 end
 if (player.dir == 2) then player.x=player.x+1 end
 if (player.dir == 3) then player.y=player.y+1 end
 if (player.dir == 4) then player.x=player.x-1 end

 if (btnp(0,player_num)) then
 player.dir = 4
 end

 if (btnp(1,player_num)) then
 player.dir = 2
 end

 if (btnp(2,player_num)) then
 player.dir = 1
 end

 if (btnp(3,player_num)) then
 player.dir = 3
 end
 
 if (player.dir != last_dir) then
  sfx(1,2,0)
  end
 
 if (pget(player.x,player.y) != 0) then
  player.dead = true
 end
end

function update_game()
 update_player(p1,1)
 update_player(p2,0)
 if (p1.dead or p2.dead) then
  show_game_over()
 end
end

function draw_game()
 pset(p1.x,p1.y,14)
 pset(p2.x,p2.y,12)
end

function show_game_over()
 scrn.upd = update_game_over
 scrn.drw = draw_game_over
 sfx(3,0,0) -- no sound on channel 0
 sfx(-1,1,0) -- no sound on channel 2
end

function update_game_over()
 if (btnp(4)) then show_menu() end
end

anim_step = 0

function draw_game_over() 
 if (p1.dead) then
  circfill(p1.x,p1.y,anim_step/5,anim_step)
 end
 
 if (p2.dead) then
  circfill(p2.x,p2.y,anim_step/5,anim_step)
 end

 anim_step += 1
 if (anim_step > 16) then
  anim_step = 0
 end
 
 --rectfill(33,46,102,79,0)
 print("game over",45,50,7)
 if (p1.dead and p2.dead) then
  print("it was a tie :(",38,60,6)
 elseif (p2.dead) then
  print("player 1 wins!",40,60,14)
 else
  print("player 2 wins!",40,60,12)
 end
 
 print("press z to reset",36,70,1)
 
end
